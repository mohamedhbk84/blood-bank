<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.contact_us.Contact_usFragment">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


   <android.support.constraint.ConstraintLayout
       android:layout_width="match_parent"
       android:layout_height="match_parent">

       <ImageView
           android:id="@+id/imageView5"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="36dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:src="@mipmap/logo_login"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toTopOf="parent" />

       <ImageView
           android:id="@+id/imageView11"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="44dp"
           android:src="@drawable/ic_phone_black"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/imageView5" />

       <TextView
           android:id="@+id/textView3"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="16dp"
           android:layout_marginLeft="16dp"
           android:layout_marginTop="208dp"
           android:gravity="center"
           android:text="@string/phone"
           android:textColor="#FF000000"
           android:textSize="16dp"
           app:layout_constraintStart_toEndOf="@+id/imageView11"
           app:layout_constraintTop_toTopOf="parent" />


       <TextView
           android:id="@+id/contact_num"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="32dp"
           android:layout_marginLeft="32dp"
           android:layout_marginTop="43dp"
           android:gravity="center"
           android:text="01029520915"
           android:textColor="#FF000000"
           android:textSize="18dp"
           app:layout_constraintStart_toEndOf="@+id/textView3"
           app:layout_constraintTop_toBottomOf="@+id/imageView5" />


       <ImageView
           android:id="@+id/imageView12"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="12dp"
           android:layout_marginBottom="8dp"
           android:src="@drawable/ic_email_black_24dp"
           app:layout_constraintBottom_toTopOf="@+id/contact_face"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/imageView11"
           app:layout_constraintVertical_bias="0.086" />

       <TextView
           android:id="@+id/textView6"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="16dp"
           android:layout_marginLeft="16dp"
           android:layout_marginTop="16dp"
           android:gravity="center"
           android:text="@string/e_mail"
           android:textColor="#FF000000"
           android:textSize="16dp"
           app:layout_constraintStart_toEndOf="@+id/imageView12"
           app:layout_constraintTop_toBottomOf="@+id/textView3" />


       <TextView
           android:id="@+id/textView12"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="16dp"
           android:layout_marginLeft="16dp"
           android:layout_marginTop="12dp"
           android:text=":"
           android:textSize="18dp"
           app:layout_constraintStart_toEndOf="@+id/textView6"
           app:layout_constraintTop_toBottomOf="@+id/contact_num" />

       <TextView
           android:id="@+id/textView7"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="44dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:text=":"
           android:textSize="17dp"
           app:layout_constraintEnd_toStartOf="@+id/contact_num"
           app:layout_constraintHorizontal_bias="0.75"
           app:layout_constraintStart_toEndOf="@+id/textView3"
           app:layout_constraintTop_toBottomOf="@+id/imageView5" />

       <TextView
           android:id="@+id/contact_email"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="12dp"
           android:text="hi4am3ly@gmail.com"
           android:textColor="#FF000000"
           android:textSize="18dp"
           app:layout_constraintStart_toEndOf="@+id/textView12"
           app:layout_constraintTop_toBottomOf="@+id/contact_num" />


       <ImageView
           android:id="@+id/contact_face"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="16dp"
           android:layout_marginLeft="16dp"
           android:layout_marginTop="32dp"
           android:src="@mipmap/face"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/textView6" />


       <ImageView
           android:id="@+id/contact_twitter"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="28dp"
           android:layout_marginLeft="28dp"
           android:layout_marginTop="32dp"
           android:src="@mipmap/twitter"
           app:layout_constraintStart_toEndOf="@+id/contact_face"
           app:layout_constraintTop_toBottomOf="@+id/textView6" />


       <ImageView
           android:id="@+id/contact_youtube"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="24dp"
           android:layout_marginLeft="24dp"
           android:layout_marginTop="32dp"
           android:src="@mipmap/youtube"
           app:layout_constraintStart_toEndOf="@+id/contact_twitter"
           app:layout_constraintTop_toBottomOf="@+id/textView6" />


       <ImageView
           android:id="@+id/contact_insta"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="28dp"
           android:layout_marginLeft="28dp"
           android:layout_marginTop="32dp"
           android:src="@mipmap/insta"
           app:layout_constraintStart_toEndOf="@+id/contact_youtube"
           app:layout_constraintTop_toBottomOf="@+id/textView6" />

       <ImageView
           android:id="@+id/contact_whats"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="264dp"
           android:layout_marginLeft="264dp"
           android:layout_marginTop="32dp"
           android:src="@mipmap/whats"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/textView6" />


       <ImageView
           android:id="@+id/contact_google"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="32dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:src="@mipmap/google"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintStart_toEndOf="@+id/contact_whats"
           app:layout_constraintTop_toBottomOf="@+id/textView6" />


       <Button
           android:id="@+id/contact_call"
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="24dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:background="@color/blue"
           android:text="@string/contact_us"
           android:textColor="@color/white"
           android:textSize="20dp"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/contact_insta" />


       <EditText
           android:id="@+id/contact_msgTitle"
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:layout_marginTop="8dp"
           android:background="@drawable/ed_login_back"
           android:hint="@string/msg_title"
           android:paddingLeft="10dp"
           android:paddingRight="10dp"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintHorizontal_bias="0.0"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/contact_call"
           />



       <EditText
           android:id="@+id/contact_msg"
           android:layout_width="match_parent"
           android:layout_height="80dp"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="8dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:background="@drawable/ed_login_back"
           android:gravity="start"
           android:hint="@string/your_msg"
           android:paddingRight="10dp"
           android:paddingLeft="10dp"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/contact_msgTitle" />


       <Button
           android:id="@+id/btn_send"
           android:layout_width="match_parent"
           android:layout_height="40dp"
           android:layout_marginStart="8dp"
           android:layout_marginLeft="8dp"
           android:layout_marginTop="16dp"
           android:layout_marginEnd="8dp"
           android:layout_marginRight="8dp"
           android:layout_marginBottom="8dp"
           android:background="@color/blue"
           android:text="@string/send"
           android:textColor="@color/white"
           android:textSize="20dp"
           app:layout_constraintBottom_toBottomOf="parent"
           app:layout_constraintEnd_toEndOf="parent"
           app:layout_constraintHorizontal_bias="0.0"
           app:layout_constraintStart_toStartOf="parent"
           app:layout_constraintTop_toBottomOf="@+id/contact_msg" />

   </android.support.constraint.ConstraintLayout>

    </ScrollView>


</FrameLayout>